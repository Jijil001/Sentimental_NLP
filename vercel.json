{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "api": {
        "runtime": "python3.9",
        "memory": 512
      }
    },
    "install": {
      "include": ["requirements.txt"]
    }
  }
  